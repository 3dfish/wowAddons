["init"] = {
["custom"] = "--checks if the unit is a valid target to be marked\naura_env.isUnitValidAndGetOrder = function (unit)\n    local guid = UnitGUID(unit)    \n    if not guid then return end\n    local npcId = select(6, strsplit(\"-\", guid))\n    if npcId and UnitExists(unit) and not GetRaidTargetIndex(unit) and not UnitIsDead(unit) then\n        --if it isn't in the custom marker list then we check the regular one and simply return true\n        return aura_env.checkCustomNpc(npcId) or aura_env.config[\"npc\".. npcId]     \n    end\nend\n\n--special case for nameplate marking, but same as the function above. Should merge them in the future\naura_env.isUnitValidNameplate = function (unit)\n    --todo\nend\n\n--takes in a unit and the order to mark it with. function checks the marks in the order to find the first availible marker from that order\naura_env.markUnit = function (unit, order)\n    WeakAuras.ScanEvents(\"AUTOMARK_REQUEST_MARK_MOB\",UnitGUID(\"player\"), unit, order)\nend\n\n\naura_env.checkCustomNpc = function (npcId)\n    for k,v in pairs(aura_env.config.npcCustom) do \n        if v.npc == npcId then \n            return {v.mark1-1 , v.mark2-1, v.mark3-1, v.mark4-1}\n        end\n    end\n    return false\nend\n\n\n\n\n",
["do_custom"] = true,
},
