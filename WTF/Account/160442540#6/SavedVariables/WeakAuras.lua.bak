pairs(aura_env.saved.deck) do\n                s[spellID] = aura_env.updateState(spellID)\n            end\n            \n            return true\n        end\n    end\n    \n    --[[ disabled until the spell is hotfixed to reset on raid pull\n    if e == \"ENCOUNTER_START\" then\n        local difficultyID = select(3, ...)\n        if difficultyID then\n            local _, groupType = GetDifficultyInfo(difficultyID)\n            if groupType and groupType == \"raid\" then\n            print(\"reset on \" .. e)\n                aura_env.reshuffle(true)\n            for spellID in pairs(aura_env.saved.deck) do\n                s[spellID] = aura_env.updateState(spellID)\n            end\n            \n            return true\n            end\n        end\n    end\n    ]]\n    \n    if e == \"PLAYER_SPECIALIZATION_CHANGED\" and not (aura_env.currentSpec == GetSpecialization()) then\n        aura_env.reshuffle(true) \n        for spellID in pairs(aura_env.saved.deck) do\n            s[spellID] = aura_env.updateState(spellID)\n        end\n        \n        return true\n    end\n    \n    if e == \"TRAIT_CONFIG_UPDATED\" then\n        aura_env.reshuffle(true)\n        for spellID in pairs(aura_env.saved.deck) do\n            s[spellID] = aura_env.updateState(spellID)\n        end\n        \n        return true\n    end\nend",
